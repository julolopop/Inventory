<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usuario.inventoryMVP">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.usuario.inventoryMVP.SplashActivity"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.usuario.inventoryMVP.ui.login.LoginActivity"
            android:theme="@style/LoginTheme" />
        <activity android:name="com.example.usuario.inventoryMVP.InventoryActivity" />
        <activity android:name="com.example.usuario.inventoryMVP.DashBoardActivity" />
        <activity android:name="com.example.usuario.inventoryMVP.ProductosActivity" />
        <activity
            android:name="com.example.usuario.inventoryMVP.DependencyActivity"
            android:theme="@style/AppTheme1" />
        <activity
            android:name="com.example.usuario.inventoryMVP.SectorActivity"
            android:theme="@style/AppTheme1" />
        <activity android:name="com.example.usuario.inventoryMVP.GeneralSettingActivity" />
        <activity android:name="com.example.usuario.inventoryMVP.AccountSettingActivity" />
        <activity android:name="com.example.usuario.inventoryMVP.ui.login.about.AboutActivity" />
        <activity android:name="com.example.usuario.inventoryMVP.AddDependencyActivity"></activity>
    </application>

</manifest>